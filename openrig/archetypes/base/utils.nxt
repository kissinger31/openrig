        "/psd_mirror_actions": {
            "comment": "This node is the base node that is used for mirror psd data.", 
            "attrs": {
                "action": {
                    "comment": "Action to use for mirror. ", 
                    "type": "raw", 
                    "value": "deltas"
                }
            }, 
            "code": [
                "mc.undoInfo(openChunk=1)", 
                "try:", 
                "    # Store Global symmetry setting, something is changing this during the mirror", 
                "    global_mirror_state = mc.symmetricModelling(symmetry=1, q=1)", 
                "    ", 
                "    # ------------------------------------", 
                "    # Get Selection", 
                "    # ------------------------------------", 
                "    ", 
                "    # Targets and their blendshapes", 
                "    #", 
                "    if '${action}' == 'deltas':", 
                "        target_connections = mm.eval('getPoseEditorTreeviewSelection(2)') ", 
                "        target_list = list()", 
                "        blendshape_list = list()", 
                "        for targ in target_connections:", 
                "            node, index = targ.split('.')", 
                "            if index:", 
                "                bs = rig_psd.getDeformer(node)", 
                "                target = mc.listConnections('{}.output[{}]'.format(node, index), p=True)", 
                "                if target:", 
                "                    target = target[0].split('.')[1]", 
                "                    target_list.append(target)", 
                "                    blendshape_list.append(bs) ", 
                "            ", 
                "    # Interpolators", 
                "    # ", 
                "    node_list = mm.eval('getPoseEditorTreeviewSelection(1)')", 
                "    ", 
                "    # ------------------------------------", 
                "    # Store pose", 
                "    # ------------------------------------", 
                "    ", 
                "    #if '{action}' == 'system':", 
                "    #    controls = rigrepo.libs.control.getControls()", 
                "    #    if controls:", 
                "    #        rigrepo.libs.control.setPoseAttr(controls, 6)", 
                "    #        # Got to bind pose", 
                "    #        rigrepo.libs.control.toPoseAttr(controls, 0)", 
                "    ", 
                "    # ------------------------------------", 
                "    # Main Loop", 
                "    # ------------------------------------", 
                "    ", 
                "    for node in node_list:", 
                "        node = rig_psd.getInterp(node)", 
                "        mirror_token = rig_common.getSideToken(node)", 
                "        if mirror_token == 'r':", 
                "            print('mirror only works for left to right'),", 
                "            continue", 
                "        ", 
                "        mirror_node = rig_common.getMirrorName(node)", 
                "        ", 
                "        if not mirror_node:", 
                "            print('PSD Mirror: Warning [{}] has no mirror system'.format(node))", 
                "            continue", 
                "        ", 
                "        # -------------------------------", 
                "        # MIRROR SYSTEM", 
                "        # -------------------------------", 
                "        ", 
                "        if '${action}' == 'system':", 
                "        ", 
                "            # Remove existing pose controls before mirror. They cause mirror errors", 
                "            if mc.objExists(mirror_node):", 
                "                mirror_pose_controls = rig_psd.getPoseControls(mirror_node)", 
                "                for pose_control in mirror_pose_controls:", 
                "                    rig_psd.removePoseControl(mirror_node, pose_control)", 
                "                    ", 
                "            pose_list = rig_common.pyListToMelArray(rig_psd.getPoseNames(node))", 
                "            try:", 
                "                mm.eval('poseInterpolatorMirror {} {} _l_ _r_ 1 1 1'.format(node, pose_list))", 
                "            except:", 
                "                traceback.print_exc()", 
                "            ", 
                "            # Attr settings", 
                "            attr_list = ['outputSmoothing', 'regularization']", 
                "            for attr in attr_list:", 
                "                value = mc.getAttr('{}.{}'.format(node, attr))", 
                "                mc.setAttr('{}.{}'.format(mirror_node, attr), value)", 
                "            ", 
                "            pose_controls = rig_psd.getPoseControls(node)", 
                "            ", 
                "            interpolation = mc.getAttr(node+'.interpolation')", 
                "            mc.setAttr(mirror_node+'.interpolation', interpolation)", 
                "", 
                "            for pc in pose_controls:", 
                "                con = mc.listConnections(pc, p=1)", 
                "                for c in con:", 
                "                    con_node = c.split('.')[0]", 
                "                    if con_node == node:", 
                "                        mirror_pose_control = rig_common.getMirrorName(pc)", 
                "                        if not mirror_pose_control in rig_psd.getPoseControls(mirror_node):", 
                "                            if mc.objExists(mirror_pose_control):", 
                "                                rig_psd.addPoseControl(mirror_node, mirror_pose_control)          ", 
                "", 
                "            data_obj = psd_data.PSDData()", 
                "            data_obj.gatherDataIterate([node])", 
                "            data_obj.applyData([node], mirror=True)", 
                "            ", 
                "            print('Mirrored Interpolator: ' + node)", 
                "       ", 
                "            ", 
                "        # -------------------------------", 
                "        # MIRROR ONLY DELTAS", 
                "        # -------------------------------", 
                "        ", 
                "        if '${action}' == 'deltas':", 
                "            ", 
                "            bs = rig_psd.getDeformer(node)", 
                "            mirror_bs = rig_psd.getDeformer(mirror_node)", 
                "            deltas_list = list()", 
                "            ", 
                "            # Get Deltas", 
                "            for pose in rig_psd.getPoseNames(node):", 
                "                if pose not in target_list:", 
                "                    continue", 
                "                index = rig_psd.getPoseShapeIndex(node, pose)", 
                "                mirror_pose = rig_common.getMirrorName(pose)", 
                "                mirror_index = rig_psd.getPoseShapeIndex(mirror_node, mirror_pose)", 
                "                ", 
                "                if index != None and mirror_index != None:", 
                "                    target_name = rig_blendShape.getTargetName(mirror_bs, mirror_index)", 
                "                    deltas, indices = rig_blendShape.getTargetDeltas(bs, index)", 
                "                    if not deltas:", 
                "                        continue", 
                "                    ", 
                "                    # Center Mirror", 
                "                    if node == mirror_node and index == mirror_index:", 
                "                        rig_blendShape.setTargetDeltas(mirror_bs, deltas, indices, mirror_index) ", 
                "                        mc.blendShape(bs, e=True, mt=(0, mirror_index), ss=1, sa='x')", 
                "                        print('Mirrored deltas: ' + pose)", 
                "                    # Side Flip    ", 
                "                    else:", 
                "                        rig_blendShape.clearTargetDeltas(mirror_bs, mirror_index)", 
                "                        rig_blendShape.setTargetDeltas(mirror_bs, deltas, indices, mirror_index) ", 
                "                        values = rig_blendShape.getTargetMapWeights(mirror_bs, mirror_pose)", 
                "                        mc.blendShape(mirror_bs, e=True, ft=(0, mirror_index), ss=1, sa='x')", 
                "                        rig_blendShape.setTargetMapWeights(mirror_bs, mirror_pose, values)", 
                "                        print('flipped deltas: ' + pose)", 
                "                    ", 
                "    # restore pose", 
                "    #if '{action}' == 'system':", 
                "    #    if controls:", 
                "    #        rigrepo.libs.control.toPoseAttr(controls, 6)", 
                "except:", 
                "    traceback.print_exc()", 
                "mc.undoInfo(closeChunk=1)", 
                "", 
                "# GLOBAL MIRROR SETTINGS", 
                "#   Setting the global mirror state is going into the undo stack", 
                "#   so undoing the mirror action is turning it back on if it was changed.", 
                "#   This shuts off undo so the mirror setting doesn't get added to ", 
                "#   the undo stack.", 
                "try:", 
                "    mc.undoInfo(stateWithoutFlush=0)", 
                "    # Restore global mirror state", 
                "    mc.symmetricModelling(symmetry=globalMirrorState)", 
                "    mc.undoInfo(stateWithoutFlush=1)", 
                "except:", 
                "    mc.undoInfo(stateWithoutFlush=1)", 
                "                ", 
                ""
            ]
        }, 
