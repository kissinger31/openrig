        "/psd_actions": {
            "comment": "This node is used to house the code for different actions that are used during editing PSD's\n", 
            "attrs": {
                "action": {
                    "comment": "Currently, the actions that can be used are as follows. (add_pose, update_pose, delete_pose)", 
                    "type": "raw", 
                    "value": "add_pose"
                }
            }, 
            "code": [
                "mc.undoInfo(openChunk=1)", 
                "try:", 
                "", 
                "    # ------------------------------------------------------------------------", 
                "    # Get selection", 
                "    # ------------------------------------------------------------------------", 
                "    ", 
                "    # Interps", 
                "    interp_list = mm.eval('getPoseEditorTreeviewSelection(1)')", 
                "    ", 
                "    # Poses", 
                "    pose_list = list()", 
                "    pose_connections = mm.eval('getPoseEditorTreeviewSelection(2)') ", 
                "    for pose_connection in pose_connections:", 
                "        interp, index = pose_connection.split('.')", 
                "        interp = rig_psd.getInterp(interp)", 
                "        if index:", 
                "            bs = rig_psd.getDeformer(interp)", 
                "            ", 
                "            pose = mc.getAttr('{}.pose[{}].poseName'.format(interp, index))", 
                "            #pose = mc.listConnections(interp+'.output['+str(index)+']', p=1)", 
                "            if pose:", 
                "                pose_list.append((interp, pose, bs))", 
                "                ", 
                "    # ------------------------------------------------------------------------", 
                "    # Add pose ", 
                "    # ------------------------------------------------------------------------", 
                "    ", 
                "    if '${action}' == 'add_pose':", 
                "        poseName = 'NEW_POSE'", 
                "        ", 
                "        for interp in interp_list:", 
                "            interp =rig_psd.getInterp(interp)", 
                "            rig_psd.addPose(interp, poseName, type='swing')", 
                "            bs = rig_psd.getDeformer(interp)", 
                "            if not bs:", 
                "                if mc.objExists('skin_psd'):", 
                "                    bs = 'skin_psd'", 
                "                else:", 
                "                    print('could not find blendShape associated with '+interp)", 
                "                    continue", 
                "            rig_psd.addShape(interp, poseName, bs=bs)", 
                "            ", 
                "    # ------------------------------------------------------------------------", 
                "    # Update pose ", 
                "    # ------------------------------------------------------------------------", 
                "    ", 
                "    if '${action}' == 'update_pose':", 
                "    ", 
                "        for pose in pose_list:", 
                "            interp, pose, bs = pose", 
                "            ", 
                "            # Update pose", 
                "            rig_psd.updatePose(interp, pose)", 
                "            ", 
                "            # Update pose control data", 
                "            #", 
                "            pose_control_data = rig_psd.getPoseControlData(interp, pose)", 
                "            if not pose_control_data:", 
                "                continue", 
                "            for data in pose_control_data:", 
                "                name, type, value = data", 
                "                value = mc.getAttr(name)", 
                "                ", 
                "                # Rotate", 
                "                if type == 8:", 
                "                    # Convert degrees to radians", 
                "                    value = [math.radians(value[0][0]), math.radians(value[0][1]), math.radians(value[0][2])]", 
                "                    rig_psd.setPoseControlData(interp, pose, name, type, value)", 
                "                    ", 
                "    # ------------------------------------------------------------------------", 
                "    # Delete Deltas", 
                "    # ------------------------------------------------------------------------", 
                "                    ", 
                "    if '${action}' == 'delete_deltas':", 
                "        for pose in pose_list:", 
                "            interp, pose, bs = pose", 
                "            rig_blendShape.clearTargetDeltas(bs, pose)", 
                "", 
                "except:", 
                "    traceback.print_exc()", 
                "mc.undoInfo(closeChunk=1)", 
                ""
            ]
        }, 
